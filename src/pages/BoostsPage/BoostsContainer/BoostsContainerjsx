import { FC } from 'react'
import { BoostsTitle } from '../BoostsTitle/BoostsTitle'
import { BoostItem } from './BoostItem/BoostItem'
import styles from './boostsContainer.module.scss'

interface props {
    handleModal: () => void
}

export const BoostsContainer: FC<props> = ({handleModal}) => {
    const boosts = [
        {   
            title: 'Free буст монет',
            text: 'х10 монет за тап в секунду на 10 секунд',
            img: 'data:image/png;base64,',
        },
        {   
            title: 'Free буст энергии',
            text: 'х10 монет за тап в секунду на 10 секунд',
            img: 'data:image/png;base64,',
        },
    ]
    return(
        <div className={styles.boosts_container}>
            <BoostsTitle title='Бесплатные бустеры'/>
            {
                boosts.map((item, index) => {
                    return <BoostItem
                        title={item.title}
                        text={item.text}
                        imgUrl={item.img}
                        key={index}
                        handleModal={handleModal}
                    />
                })
            }
        </div>
    )
}